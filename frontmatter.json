{
  "$schema": "https://beta.frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "SEO: Title",
          "name": "seotitle",
          "type": "string"
        },
        {
          "title": "SEO: Description",
          "name": "seodesc",
          "type": "string"
        },
        {
          "title": "SEO: Robots",
          "name": "seorobots",
          "type": "choice",
          "choices": [
            "",
            "index, follow",
            "noindex, follow",
            "noindex, nofollow"
          ]
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Authors",
          "name": "authors",
          "type": "taxonomy",
          "taxonomyId": "authors"
        }
      ]
    }
  ],
  "frontMatter.telemetry.disable": true,
  "frontMatter.framework.id": "11ty",
  "frontMatter.preview.host": "http://localhost:8080",
  "frontMatter.content.fmHighlight": false,
  "frontMatter.content.pageFolders": [
    {
      "title": "pages",
      "path": "[[workspace]]/src/pages"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/src/blog"
    },
    {
      "title": "recipes",
      "path": "[[workspace]]/src/Recipes"
    }
  ],
  "frontMatter.taxonomy.customTaxonomy": [
    {
      "id": "authors",
